@using Revas.Models
 @model Portfolio

<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="ImgFile">ImgFile</label>
            <input asp-for="ImgFile" class="form-control">
            <span asp-validation-for="ImgFile" style="color:red"></span>
        </div>
         
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
 </div>